{
  "rewrites": [
    {
      "source": "/insights",
      "destination": "https://ghost.talentlean.com/"
    },
    {
      "source": "/insights/(.*)",
      "destination": "https://ghost.talentlean.com/$1"
    }
  ],

  "redirects": [
    {
      "source": "https://insights.talentlean.com/:path*",
      "destination": "https://talentlean.com/insights/:path*",
      "permanent": true
    }
  ]
}
