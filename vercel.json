{
  "rewrites": [
    {
      "source": "/insights",
      "destination": "https://ghost.talentlean.com/"
    },
    {
      "source": "/insights/:path*",
      "destination": "https://ghost.talentlean.com/:path*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/insights/",
      "destination": "/insights",
      "permanent": false
    }
  ]
}