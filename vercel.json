{
  "routes": [
    {
      "src": "/insights/?",
      "dest": "https://ghost.talentlean.com/insights"
    },
    {
      "src": "/insights/(.*)",
      "dest": "https://ghost.talentlean.com/insights/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "insights.talentlean.com"
        }
      ],
      "destination": "https://talentlean.com/insights",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "insights.talentlean.com"
        }
      ],
      "destination": "https://talentlean.com/insights/:path*",
      "permanent": true
    }
  ]
}
