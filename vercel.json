{
  "rewrites": [
    {
      "source": "/insights",
      "destination": "https://ghost.talentlean.com/"
    },
    {
      "source": "/insights/:path*",
      "destination": "https://ghost.talentlean.com/:path*"
    }
  ],
  "routes": [
    {
      "src": "/insights(?:/.*)?",
      "dest": "/insights"
    }
  ]
}